<table>
<tr>
<th>firstname</th>
<th>lastname</th>
<th>email</th>
<th></th>
</tr>
<% @file.each do |b| %>
	<tr>
	<td><%= b.firstname%></td>
<td><%= b.lastname%></td>
<td><%= b.email%></td>
<td> <%= link_to 'Edit',first_edit_path(b)%></td>
<td> <%= link_to 'Destroy',first_destroy_path(b),method: :delete, data: {confirm: 'are you sure?'} %></td>
</tr>
<%end%>
</table>