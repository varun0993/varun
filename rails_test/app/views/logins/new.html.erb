<h1>Sign Up Here</h1>

 <% if @log.errors.any? %>
   <div class="error_messages">
     <h2>Form is invalid</h2>
     <ul>
       <% for message in @login_create_path.errors.full_messages %>
         <li><%= message %></li>
       <% end %>
     </ul>
   </div>
 <% end %>
 <div class="container">
 

	<%= form_for @log, url: login_new_path do |f| %>
	
      <p>
    <%= f.label :username%>
      <%= f.text_field :username%></br>
    </p>
    <p>
    <%= f.label :password%>
      <%= f.text_field :password%></br>
    </p>
	 
	 </div>
<% end %>	

 <%= f.submit :Submit,:class=>"btn btn-success" %>
 <%= f.submit :Cancel,:class=>"btn btn-success" %>
 <% end %>